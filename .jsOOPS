//OOPS in JavaScript

class Person{
    Person(name, age){
        this.name = name
        this.age = age
    }

    greet(){
        console.log("Hey! Myself Arth Vala. Good to See You.")
    }
}

class Student extends Person{
    Student(name, age, grade){
        super.name = name
        super.age = age
        this.grade = grade
    }

    displayData(){
        console.log(`Name : ${this.name} | Age : ${this.age} | Grade : ${this.grade}`)
    }
}


const p1 = new Student("John", 20, 85)
p1.displayData
